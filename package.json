{
    "name": "dash_blueprint",
    "version": "0.0.1",
    "description": "Dash wrapper for Blueprint.js",
    "main": "build/index.js",
    "scripts": {
        "start": "webpack-serve --port 55556 --config ./webpack.serve.config.js --open",
        "validate-init": "python _validate_init.py",
        "prepublishOnly": "npm run validate-init",
        "build:js": "webpack --mode production",
        "build:backends": "dash-generate-components ./src/lib/components dash_blueprint -p package-info.json --r-prefix '' --jl-prefix ''",
        "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
        "build": "npm run build:js && npm run build:backends",
        "build:activated": "npm run build:js && npm run build:backends-activated",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "author": "Ben Weise <ben.weise@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@babel/plugin-proposal-decorators": "^7.14.5",
        "@blueprintjs/core": "^3.46.0",
        "@blueprintjs/datetime": "^3.23.5",
        "@blueprintjs/icons": "^3.27.0",
        "@blueprintjs/select": "^3.16.5",
        "@data-ui/sparkline": "^0.0.84",
        "@data-ui/theme": "^0.0.8",
        "d3": "^7.0.0",
        "ramda": "^0.26.1",
        "resize-observer-polyfill": "^1.5.1"
    },
    "devDependencies": {
        "@babel/core": "^7.5.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
        "@babel/preset-env": "^7.5.4",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^6.3.0",
        "@storybook/addon-essentials": "^6.3.0",
        "@storybook/addon-links": "^6.3.0",
        "@storybook/preset-create-react-app": "^3.1.7",
        "@storybook/react": "^6.3.0",
        "babel-eslint": "^10.0.2",
        "babel-loader": "^8.0.6",
        "copyfiles": "^2.1.1",
        "css-loader": "^3.0.0",
        "eslint": "^6.0.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-react": "^7.14.2",
        "npm": "^6.1.0",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-docgen": "^4.1.1",
        "react-dom": "^16.8.6",
        "style-loader": "^0.23.1",
        "styled-jsx": "^3.2.1",
        "webpack": "^4.36.1",
        "webpack-cli": "^3.3.6",
        "webpack-serve": "^3.1.0"
    },
    "engines": {
        "node": ">=8.11.0",
        "npm": ">=6.1.0"
    }
}
