{
    "name": "dash_blueprint",
    "version": "0.0.1",
    "description": "Dash wrapper for blueprint package",
    "main": "build/index.js",
    "scripts": {
        "start": "webpack-serve ./webpack.serve.config.js --open",
        "validate-init": "python _validate_init.py",
        "prepublish": "npm run validate-init",
        "build:js-dev": "webpack --mode development",
        "build:js": "webpack --mode production",
        "build:py": "node ./extract-meta.js src/lib/components > dash_blueprint/metadata.json && copyfiles package.json dash_blueprint && python -c \"import dash; dash.development.component_loader.generate_classes('dash_blueprint', 'dash_blueprint/metadata.json')\"",
        "build:all": "npm run build:js & npm run build:js-dev & npm run build:py"
    },
    "author": "Benjamin Weise ben.weise@gmail.com",
    "license": "Apache-2.0",
    "dependencies": {
        "@blueprintjs/icons": "^3.3.0",
        "babel-preset-stage-2": "^6.24.1",
        "ramda": "^0.25.0"
    },
    "devDependencies": {
        "@blueprintjs/core": "^3.9.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "copyfiles": "^2.0.0",
        "css-loader": "^0.28.11",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-react": "^7.9.1",
        "npm": "^6.1.0",
        "react": ">=0.14",
        "react-docgen": "^2.20.1",
        "react-dom": ">=0.14",
        "style-loader": "^0.21.0",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.1",
        "webpack-serve": "^1.0.2"
    },
    "engines": {
        "node": ">=8.11.0",
        "npm": ">=6.1.0"
    }
}
