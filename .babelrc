{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "env": {
      "production": {
          "plugins": [
              "@babel/plugin-proposal-object-rest-spread", 
              "styled-jsx/babel",
              ["@babel/plugin-proposal-decorators", { "legacy": true }]
        ]
      },
      "development": {
          "plugins": [
              "@babel/plugin-proposal-object-rest-spread", 
              "styled-jsx/babel",
              ["@babel/plugin-proposal-decorators", { "legacy": true }]
            ]
      },
      "test": {
          "plugins": [
            "@babel/plugin-proposal-object-rest-spread", 
            "styled-jsx/babel",
            ["@babel/plugin-proposal-decorators", { "legacy": true }]
        ]
      }
  }
}
